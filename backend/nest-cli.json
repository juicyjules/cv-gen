{
    "$schema": "https://json.schemastore.org/nest-cli",
    "collection": "@nestjs/schematics",
    "sourceRoot": "src",
    "compilerOptions": {
      "deleteOutDir": true,
      "webpack": false, 
      "tsConfigPath": "tsconfig.build.json", 
      "assets": [
        
        
        
  
        
        {
          "include": "generated/prisma/client/**/*", 
          "outDir": "dist/src", 
          "watchAssets": false 
        },
        
        {
          "include": "generated/typegraphql-prisma/**/*",
          "outDir": "dist/src",
          "watchAssets": false
        }
        
        
      ],
      "watchAssets": true 
    }
  }